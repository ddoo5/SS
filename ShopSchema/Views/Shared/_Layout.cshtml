<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Shop</title>
    <partial name="Models/_Styles"/>
</head>
<body>

   <partial name="Models/_Header"/>

<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

    <partial name="Models/_Footer"/>

    <partial name="Models/_Scripts"/>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>